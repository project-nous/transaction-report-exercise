{
  "name": "express-api",
  "version": "1.0.0",
  "description": "My api",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev:server": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "dev": "concurrently \"yarn dev:server\" \"cd web && yarn dev\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "express": "^4.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3",
    "nodemon": "^3.1.9"
  }
}
